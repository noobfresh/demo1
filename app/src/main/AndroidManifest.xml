<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.demo1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.Activity.MainActivity"
            android:label="news">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.Activity.NewsActivity" />
        <activity
            android:name=".view.Activity.FirstSettingActivity"
            android:theme="@style/ThemeActivity" />
        <activity
            android:name=".view.Activity.FirstFirstActivity"
            android:theme="@style/ThemeActivity" />
        <activity android:name=".view.Activity.FirstSecondActivity"
            android:theme="@style/ThemeActivity"/>
        <activity android:name=".view.Activity.FirstThirdActivity"
            android:theme="@style/ThemeActivity"/>
        <activity android:name=".view.Activity.SecondSettingActivity"
            android:theme="@style/ThemeActivity"/>
        <activity android:name=".view.Activity.SecondFirstActivity"
            android:theme="@style/ThemeActivity"/>
        <activity android:name=".view.Activity.SecondSecondActivity"
            android:theme="@style/ThemeActivity"/>
        <activity android:name=".view.Activity.SecondThirdActivity"
            android:theme="@style/ThemeActivity"/>
    </application>

</manifest>